# remove from airootfs!
[Trigger]
Operation = Install
Operation = Upgrade
Type = Package
Target = linux-xanmod-cacule-uksm-cjktty

[Action]
Description = Renaming linux-xanmod-cacule-uksm-cjktty to linux...
When = PostTransaction
Depends = linux-xanmod-cacule-uksm-cjktty
Exec = /usr/bin/sh -c 'mv -v /boot/vmlinuz-linux-xanmod-cacule-uksm-cjktty /boot/vmlinuz-linux; mv -v /boot/initramfs-linux-xanmod-cacule-uksm-cjktty.img /boot/initramfs-linux.img'
